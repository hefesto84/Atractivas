<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<!-- Fondo -->
	<item>
		<shape android:shape="rectangle">
			<solid android:color="#ffeb6c99"/>
		</shape>
	</item>
	<!-- degradado -->
	<item
		android:bottom="1dp"
	>
		<shape android:shape="rectangle">
			<gradient
				android:angle="-90"
				android:startColor="#fffff0f5"
				android:endColor="#fffff0f5"
				/>
		</shape>
	</item>
</layer-list>
