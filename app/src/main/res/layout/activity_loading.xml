<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"			 xmlns:tools="http://schemas.android.com/tools"			 android:layout_width="match_parent"			 android:layout_height="match_parent"			 android:background="#ffffff"			 tools:context="es.ubiqua.atractivas.LoadingActivity">	<RelativeLayout		android:layout_width="match_parent"		android:layout_height="match_parent"		android:padding="0dp">		<org.skilladev.ui.ImageViewAspectRatio			android:layout_width="match_parent"			android:layout_height="wrap_content"			android:layout_margin="0dp"			android:id="@+id/splash"			android:scaleType="fitCenter"			android:gravity="center"			android:layout_gravity="center"			android:keepScreenOn="true"			android:layout_centerInParent="true"			android:src="@drawable/blank"			/>        <TextView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:id="@+id/produced"            />	</RelativeLayout></FrameLayout>